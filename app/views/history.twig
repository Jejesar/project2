{% extends "layout.twig" %}

{% block scripts %}
	<script defer src="/javascripts/history.js"></script>
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" crossorigin="anonymous">
{% endblock %}

{% block body %}

	{# <div class="container"> #}
	<div class="card mb-4 mx-lg-4">
		<h1 class="card-header text-center">List of all sequences</h1>
		<div class="card-body p-0">
			<table class="table table-striped table-hover border rounded m-0" id="table">
				<thead>
					<tr>
						<th scope="col">ID</th>
						<th scope="col">Name</th>
						<th scope="col">Created at</th>
						<th scope="col">Nbr of 0cm</th>
						<th scope="col">Nbr of 1cm</th>
						<th scope="col">Nbr of 2cm</th>
						<th scope="col">Nbr of 3cm</th>
						<th scope="col">Nbr of 4cm</th>
						<th scope="col">Nbr of 5cm</th>
						<th scope="col">Comments</th>
						<th scope="col">Edit</th>
					</tr>
				</thead>
				<tbody id="table-list-sequences"></tbody>
			</table>
		</div>
	</div>

	{# </div> #}


{% endblock %}

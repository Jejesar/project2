{% extends "layout.twig" %}

{% block scripts %}
	<script defer src="/javascripts/history.js"></script>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
{% endblock %}

{% block body %}

	<div class="container">
		<div class="card mb-4">
			<h1 class="card-header text-center">List of all sequences</h1>
			<div class="card-body p-0">
				<table class="table table-striped table-hover border rounded m-0">
					<thead>
						<tr>
							<th scope="col">ID</th>
							<th scope="col">Name</th>
							<th scope="col">Created at</th>
							<th scope="col">Nbr of 0cm</th>
							<th scope="col">Nbr of 2cm</th>
							<th scope="col">Nbr of 4cm</th>
							<th scope="col">Nbr of 6cm</th>
							<th scope="col">Comments</th>
							<th scope="col">Edit</th>
						</tr>
					</thead>
					<tbody id="table-list-sequences"></tbody>
				</table>
			</div>
		</div>

	</div>


{% endblock %}
